{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-zend-opcache": "*",
        "ausi/slug-generator": "^1.1",
        "beberlei/doctrineextensions": "^1.2",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "giggsey/libphonenumber-for-php": "^8.10",
        "guzzlehttp/guzzle": "^6.3",
        "hackzilla/password-generator": "^1.4",
        "hautelook/phpass": "^1.1",
        "hybridauth/hybridauth": "~3.0",
        "league/glide-symfony": "^1.0",
        "league/omnipay": "^3",
        "minwork/array": "^1.3",
        "nesbot/carbon": "^2.16",
        "omnipay/paypal": "^3.0",
        "pagerfanta/pagerfanta": "^2.1",
        "paragonie/sodium_compat": "^1.10",
        "paypal/rest-api-sdk-php": "^1.14",
        "psr/simple-cache": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",
        "stefano/stefano-tree": "^4.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/lock": "*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/property-info": "*",
        "symfony/security-bundle": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "ticketswap/omnipay-przelewy24": "^2.0",
        "twig/twig": "^2.0",
        "webmozart/path-util": "^2.3",
        "willdurand/js-translation-bundle": "^2.6",
        "zendframework/zend-feed": "^2.12"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "^5.0",
        "stesie/phpcs-doctrine-annotation-rules": "^1.2",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%/asset": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "public-dir": "../"
    }
}
