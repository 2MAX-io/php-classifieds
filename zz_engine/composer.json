{
    "name": "2max/php_classified_ads",
    "description": "php-classified-ads.2max.io - 2MAX.io PHP Classified Ads",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-zend-opcache": "*",
        "ausi/slug-generator": "^1.1",
        "beberlei/doctrineextensions": "^1.2",
        "dompdf/dompdf": "^1.0",
        "friendsofsymfony/jsrouting-bundle": "^2.6",
        "giggsey/libphonenumber-for-php": "^8.12",
        "guzzlehttp/guzzle": "^6.5",
        "hackzilla/password-generator": "^1.5",
        "hautelook/phpass": "^1.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "hybridauth/hybridauth": "^3.3",
        "league/glide-symfony": "^1.0",
        "league/omnipay": "^3.0",
        "minwork/array": "^1.13",
        "misd/linkify": "^1.1",
        "nelmio/js-logger-bundle": "^1.6",
        "nesbot/carbon": "^2.35",
        "omnipay/paypal": "^3.0",
        "pagerfanta/pagerfanta": "^2.1",
        "paragonie/sodium_compat": "^1.13",
        "psr/simple-cache": "^1.0",
        "sensio/framework-extra-bundle": "^5.5",
        "stefano/stefano-tree": "^4.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.7",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/lock": "*",
        "symfony/mailer": "4.4.*",
        "symfony/messenger": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/polyfill-uuid": "^1.17",
        "symfony/process": "4.4.*",
        "symfony/property-info": "*",
        "symfony/security-bundle": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/yaml": "4.4.*",
        "ticketswap/omnipay-przelewy24": "^2.0",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/twig": "^3.0",
        "webmozart/path-util": "^2.3",
        "willdurand/js-translation-bundle": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "fzaninotto/faker": "^1.9",
        "phpro/grumphp": "^1.3",
        "phpstan/phpstan-doctrine": "^0.12.32",
        "phpstan/phpstan-symfony": "^0.12.20",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-symfony": "^2.1",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "^5.0",
        "stesie/phpcs-doctrine-annotation-rules": "^1.2",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.19",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "vimeo/psalm": "^4.7",
        "weirdan/doctrine-psalm-plugin": "^1.0"
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%/asset": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "public-dir": "../"
    }
}
