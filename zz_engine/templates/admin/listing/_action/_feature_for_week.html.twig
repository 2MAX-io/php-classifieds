<form
        method="post"
        action="{{ path('app_admin_listing_feature_for_week', {'id': listing.id}) }}"
        data-confirm-message="{{ 'trans.Feature for week' | trans }}: {{ listing.title }}?"
        class="d-inline js__confirm"
>
    <input type="hidden" name="_method" value="PATCH">
    <input type="hidden" name="_token" value="{{ csrf_token('csrf_adminFeatureForWeekListing' ~ listing.id) }}">
    <button class="btn btn-success mb-1">{{ 'trans.Feature for week' | trans }}</button>
</form>
