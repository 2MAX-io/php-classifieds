<form
        method="post"
        action="{{ path('app_admin_listing_raise', {'id': listing.id}) }}"
        onsubmit="return confirm('{{ 'trans.Raise' | trans }}: {{ listing.title }}?');"
        class="d-inline"
>
    <input type="hidden" name="_method" value="PATCH">
    <input type="hidden" name="_token" value="{{ csrf_token('adminRaise' ~ listing.id) }}">
    <button class="btn btn-success btn-mini">{{ 'trans.Raise' | trans }}</button>
</form>
