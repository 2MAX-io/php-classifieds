{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-zend-opcache": "*",
        "ausi/slug-generator": "^1.1",
        "beberlei/doctrineextensions": "^1.2",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "giggsey/libphonenumber-for-php": "^8.10",
        "guzzlehttp/guzzle": "^6.3",
        "hackzilla/password-generator": "^1.4",
        "hautelook/phpass": "^1.1",
        "hybridauth/hybridauth": "~3.0",
        "league/glide-symfony": "^1.0",
        "league/omnipay": "^3",
        "minwork/array": "^1.3",
        "nesbot/carbon": "^2.16",
        "omnipay/paypal": "^3.0",
        "pagerfanta/pagerfanta": "^2.1",
        "paragonie/sodium_compat": "^1.10",
        "paypal/rest-api-sdk-php": "^1.14",
        "psr/simple-cache": "^1.0",
        "sensio/framework-extra-bundle": "^5.2",
        "stefano/stefano-tree": "^4.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/lock": "*",
        "symfony/mailer": "*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/property-info": "*",
        "symfony/security-bundle": "*",
        "symfony/security-csrf": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*",
        "ticketswap/omnipay-przelewy24": "^2.0",
        "webmozart/path-util": "^2.3",
        "willdurand/js-translation-bundle": "^2.6",
        "zendframework/zend-feed": "^2.12"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%/asset": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "public-dir": "../"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "^5.0",
        "stesie/phpcs-doctrine-annotation-rules": "^1.2",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0"
    }
}
